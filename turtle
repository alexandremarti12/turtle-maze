import turtle

turtle.tracer(0, 0) # This speeds things up, but you can't see what you are doing.
turtle.pencolor("black")
turtle.pensize(3)
turtle.penup()
turtle.goto(0,-180)
turtle.pendown()
turtle.speed(10)
turtle.setheading(180)
turtle.fd(180)
turtle.setheading(90)
turtle.fd(60)
turtle.setheading(0)
turtle.fd(120)
turtle.backward(120)
turtle.setheading(90)
turtle.fd(300)
turtle.setheading(0)
turtle.fd(120)
turtle.setheading(-90)
turtle.fd(120)
turtle.setheading(180)
turtle.fd(60)
turtle.setheading(90)
turtle.fd(60)
turtle.setheading(-90)
turtle.fd(120)
turtle.setheading(90)
turtle.fd(60)
turtle.setheading(0)
turtle.fd(120)
turtle.setheading(-90)
turtle.fd(60)
turtle.setheading(0)
turtle.fd(60)
turtle.setheading(-90)
turtle.fd(60)
turtle.backward(60)
turtle.setheading(0)
turtle.fd(60)
turtle.setheading(90)
turtle.fd(60)
turtle.penup()
turtle.setheading(180)
turtle.fd(60)
turtle.pendown()
turtle.setheading(90)
turtle.fd(60)
turtle.setheading(180)
turtle.fd(60)
turtle.setheading(90)
turtle.fd(60)
turtle.setheading(0)
turtle.fd(120)
turtle.setheading(-90)
turtle.fd(60)
turtle.backward(60)
turtle.setheading(0)
turtle.fd(60)
turtle.setheading(-90)
turtle.fd(240)
turtle.setheading(180)
turtle.fd(60)
turtle.setheading(-90)
turtle.fd(60)
turtle.setheading(180)
turtle.fd(120)
turtle.setheading(90)
turtle.fd(60)
turtle.setheading(180)
turtle.fd(60)
turtle.setheading(90)
turtle.fd(60)
turtle.backward(60)
turtle.setheading(180)
turtle.fd(60)
turtle.penup()
turtle.setheading(0)
turtle.fd(300)
turtle.pendown()
turtle.setheading(-90)
turtle.fd(120)
turtle.setheading(180)
turtle.fd(120)
turtle.ht()
turtle.update()

turtle.tracer(1, 1)
turtle.pencolor("blue")
turtle.pensize(5)
turtle.penup()
turtle.goto(30,-200)
turtle.pendown()
turtle.right(90)

#Enters the maze
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(75)
turtle.left(90)
turtle.forward(125)
turtle.right(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(75)
turtle.right(90)
turtle.forward(75)
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(75)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(100)
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(125)

turtle.done()



















